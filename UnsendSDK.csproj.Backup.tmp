<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>UnsendSDK</PackageId>
    <Version>1.0.1</Version>
    <Authors>Thiago Praxedes</Authors>
    <Company>Thiago Praxedes</Company>
    <Description>A C# SDK for Unsend API</Description>
    <PackageTags>Unsend, SDK, Email, API</PackageTags>
    <RepositoryUrl>https://github.com/tpraxedes/UnsendSDK</RepositoryUrl>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>$(AssemblyName)</Title>
    <Authors>Thiago Praxedes</Authors>
    <Description>SDK for usage with Unsend</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>